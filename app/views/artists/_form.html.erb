<%= form_with(model: artist, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, required: true, placeholder: "Name" %>
  </div>

  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image, required: true %>
  </div>

<button class="ui primary button">
  Save
</button>
<% end %>
